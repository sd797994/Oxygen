{
  "Oxygen": {
    "ServerPort": 80,
    "Mesh": {
      //不采用任何mesh
      "None": {
        "Open": true, //只能打开一种mesh模式，同时需要yaml文件配合，同时确保所有host采用的mesh一致
        "ProtocolType": 1 //0 = tcp模式 1 = http1.1模式 2 = http/2模式
      },
      //基于dapr的mesh
      "Dapr": {
        "Open": false,
        "ProtocolType": 1, //仅支持http1.1
        "OpenActor": true //开启actor模式，需要提前准备dapr-state-store
      },
      //基于istio的mesh
      "Istio": {
        "Open": false,
        "ProtocolType": 2, //支持http1.1和http/2
        "CustomHeader": "canaryver" //支持金丝雀header传递
      }
    }
  }
}